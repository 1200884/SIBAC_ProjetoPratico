rule "r1:Acaros vermelhos nas abelhas"
@CF(-0.75)
lock-on-active true
when
	$e: Evidence( description == "Acaros vermelhos nas abelhas", value == "true" )
then
	Hypothesis $h = (Hypothesis)TrackingAgendaListener.getFactRef(Hypothesis.class, "varroa", "true");
	if($h != null){
		$h.update();
		$h.updateConfidence(-0.75);
 	$e.setCF(-0.75);
	        update($e);

	}
end

